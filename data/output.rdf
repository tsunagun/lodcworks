        <dcterms:relation rdf:resource="http://purl.org/net/mdlab/data/lodc/2012/a005"/>
        <dcterms:relation rdf:resource="http://purl.org/net/mdlab/data/lodc/2012/d020"/>
        <dcterms:title>LSJ2013 〜 サブカルチャー基盤データの応用 〜</dcterms:title>
        <rdfs:seeAlso rdf:resource="http://lod.sfc.keio.ac.jp/challenge2012/show_status.php?id=i012"/>
        <foaf:homepage rdf:resource="http://lod.sfc.keio.ac.jp/challenge2012/dat/idea/i012_20130120202310_lod_challenge_2012_idea.pdf"/>
      </lodc:Work>
    </dcterms:relation>
